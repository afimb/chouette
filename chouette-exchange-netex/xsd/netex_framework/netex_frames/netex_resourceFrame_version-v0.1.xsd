<xsd:schema xmlns="http://www.netex.org.uk/netex" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:siri="http://www.siri.org.uk/siri" targetNamespace="http://www.netex.org.uk/netex" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.1" id="netex_resourceFrame_version">
	<xsd:include schemaLocation="netex_commonFrame_version-v0.1.xsd"/>
	<xsd:include schemaLocation="../netex_reusableComponents/netex_transportOrganisation_version-v0.1.xsd"/>
	<xsd:include schemaLocation="../netex_all_objects_framework-v0.1.xsd"/>
	<xsd:include schemaLocation="netex_resourceFrame_support-v0.1.xsd"/>
	<xsd:annotation>
		<xsd:appinfo>
